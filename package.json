{
  "name": "@moonstone1258/vue3-verification-input",
  "version": "1.0.0",
  "description": "A feature-rich Vue 3 verification code input component with TypeScript support, auto-focus, paste handling, and success/error animations",
  "type": "module",
  "main": "./dist/vue3-verification-input.umd.js",
  "module": "./dist/vue3-verification-input.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/vue3-verification-input.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/vue3-verification-input.umd.js"
      }
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "vue",
    "vue3",
    "verification-code",
    "verification-input",
    "otp",
    "otp-input",
    "pin-code",
    "code-input",
    "typescript",
    "vue-component"
  ],
  "author": "moonstone1258",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moonstone1258/vue3-verification-input.git"
  },
  "homepage": "https://github.com/moonstone1258/vue3-verification-input#readme",
  "bugs": {
    "url": "https://github.com/moonstone1258/vue3-verification-input/issues"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "build-only": "vite build --config vite.config.lib.ts",
    "type-check": "vue-tsc --build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.1",
    "@iconify/json": "^2.2.420",
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.1",
    "@unocss/eslint-plugin": "^66.5.10",
    "@unocss/preset-attributify": "^66.5.10",
    "@unocss/preset-wind3": "^66.5.10",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.1.0",
    "npm-run-all2": "^8.0.4",
    "typescript": "~5.9.0",
    "unocss": "^66.5.10",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue": "^3.5.25",
    "vue-tsc": "^3.1.5"
  }
}
